'scopeName': 'source.skript'
'fileTypes': [
  'sk'
]
'name': 'Skript'
patterns: [
  {
    include: "#numbers"
  }
  {
    include: "#keywords"
  }
  {
    include: "#strings"
  }
  {
    include: "#variables"
  }
  {
    include: "#comments"
  }
]
repository:
  strings:
    patterns: [
      {
        begin: '"'
        end: '("|\\n)'
        name: "string.text.skript"
        patterns: [
          {
            match: "\\\\(x\\h{2}|[0-2][0-7]{,2}|3[0-6][0-7]|37[0-7]?|[4-7][0-7]?|.)"
            name: "constant.character.escape.skript"
          }
          {
            match: "(&|§)0"
            name: "keyword.type.mccolor.black.skript"
          }
          {
            match: "(&|§)1"
            name: "keyword.type.mccolor.dark_blue.skript"
          }
          {
            match: "(&|§)2"
            name: "keyword.type.mccolor.dark_green.skript"
          }
          {
            match: "(&|§)3"
            name: "keyword.type.mccolor.dark_aqua.skript"
          }
          {
            match: "(&|§)4"
            name: "keyword.type.mccolor.dark_red.skript"
          }
          {
            match: "(&|§)5"
            name: "keyword.type.mccolor.dark_purple.skript"
          }
          {
            match: "(&|§)6"
            name: "keyword.type.mccolor.gold.skript"
          }
          {
            match: "(&|§)7"
            name: "keyword.type.mccolor.gray.skript"
          }
          {
            match: "(&|§)8"
            name: "keyword.type.mccolor.dark_gray.skript"
          }
          {
            match: "(&|§)9"
            name: "keyword.type.mccolor.blue.skript"
          }
          {
            match: "(&|§)(a|A)"
            name: "keyword.type.mccolor.green.skript"
          }
          {
            match: "(&|§)(b|B)"
            name: "keyword.type.mccolor.aqua.skript"
          }
          {
            match: "(&|§)(c|C)"
            name: "keyword.type.mccolor.red.skript"
          }
          {
            match: "(&|§)(d|D)"
            name: "keyword.type.mccolor.light_purple.skript"
          }
          {
            match: "(&|§)(e|E)"
            name: "keyword.type.mccolor.yellow.skript"
          }
          {
            match: "(&|§)(f|F)"
            name: "keyword.type.mccolor.white.skript"
          }
          {
            match: "(&|§)(l|L)"
            name: "keyword.type.mccolor.bold.skript"
          }
          {
            match: "(&|§)(m|M)"
            name: "keyword.type.mccolor.strikethrough.skript"
          }
          {
            match: "(&|§)(n|N)"
            name: "keyword.type.mccolor.underline.skript"
          }
          {
            match: "(&|§)(o|O)"
            name: "keyword.type.mccolor.italic.skript"
          }
          {
            begin: '%'
            end: '(%|"|\\n)'
            name: "variable.exprwrap.skript"
          }
          {
            match: "{@[a-zA-Z0-9]+}"
            name: "variable.type.option"
          }
        ]
      }
    ]
  variables:
    patterns: [
      {
        begin: '%'
        end: '(%|"|\\n)'
        name: "variable.exprwrap.skript"
      }
      {
        match: "{@[a-zA-Z0-9]+}"
        name: "variable.type.option.skript"
      }
      {
        begin: '{_'
        end: '(}|\\n)'
        name: "variable.temp.skript"
      }
      {
        begin: '{'
        end: '(}|\\n)'
        name: "variable.normal.skript"
      }
    ]
  comments:
    patterns: [
      {
        begin: "(#!)"
        beginCaptures:
          1:
            name: "comment.mark.skript"
        end: "(!#|\\n)"
        name: "comment.mark.skript"
      }
      {
        begin: "(#)"
        beginCaptures:
          1:
            name: "comment.hash.skript"
        end: "\\n"
        name: "comment.hash.skript"
      }
    ]
  numbers:
    patterns: [
      {
        match: "\\b(?i)([0-9]+)\\b"
        name: "constant.numeric.skript"
      }
    ]
  keywords:
    patterns: [
      {
        match: "\\b(?i)(tick|(milli)?second|minute|hour|day|week|month|year|now)(s)?\\b"
        name: "keyword.type.timespan.skript"
      }
      {
        match: "\\b(?i)(trigger|permission: .+|description: .+|usage: .+|aliases: .+|function |command \/[a-zA-Z]+|(s(k|c)ript )?options)\\b"
        name: "keyword.type.cmd.skript"
      }
      {
        match: "\\b(?i)((north |south )?(north|east|south|west)|(back|down)wards|top|upper|front|horizontal|behind|direction|towards|forward(s)?|above|over|up|down|below|under|beneath)\\b"
        name: "keyword.type.direction.skript"
      }
      {
        match: "loop-(block|item|index|value|num(ber)?|player|entity|integer|attacker|victim|shooter|world|chunk|projectile|button)('s|s|-)?"
        name: "keyword.type.loop.skript"
      }
      {
        match: "event-(block|item|index|value|num(ber)?|player|entity|integer|attacker|victim|shooter|world|chunk|projectile|button)('s|s)?"
        name: "keyword.type.event.skript"
      }
      {
        match: "\\b(?i)((fire |blast |projectile )?protection|feather falling|respiration|aqua affinity|thorns|depth strider|frost walker|sharpness|smite|bane of arthropods|knockback|fire aspect|looting|efficiency|silk touch|unbreaking|fortune|power|punch|flame|infinity|luck of the sea|lure|mending)\\b"
        name: "keyword.type.enchant.skript"
      }
      {
        match: "\\b(?i)(nausea|night vision|invisibility|leaping|fire resistance|water breathing|harming|poison|regeneration|strength|(swift|weak|slow|blind)ness|(bad )?luck|jump boost|levitat(e|ion))\\b"
        name: "keyword.type.potion.skript"
      }
      {
        match: "\\b(?i)((dark )?(blue|green|aqua|red|gray)|(dark|light) purple|gold|black|yellow|white)\\b"
        name: "keyword.type.color.skript"
      }
      {
        match: "\\b(?i)(if|else|if else|loop|while|every)\\b"
        name: "keyword.else.skript"
      }

      {
        begin: '%'
        end: '(%|"|\\n)'
        name: "variable.exprwrap.skript"
      }
      {
        match: "\\b(?i)((play|attack|shoot|send|damag|serv)er|victim|(targeted )?entity|executor|console)('s|s)?\\b"
        name: "keyword.type.player.skript"
      }
      {
        match: "\\b(?i)(bat|chicken|mooshroom|pig|squid|sheep|(cave )?spider|ender(man|mite)|blaze|creeper|(elder )?guardian|ghast|magma cube|zombie( pigman)?|silverfish|slime|(chicken|spider) jockey|witch|(wither )?skeleton|(zombie )?villager|donkey|mule|ocelot|wolf|(iron|snow) golem|ender( )?dragon|wither|shulker|giant|(killer )?rabbit|(zombie |skeleton )?horse)('s|s)?\\b"
        name: "keyword.type.entity.skript"
      }

      {
        match: "(&|§)0"
        name: "keyword.type.mccolor.black.skript"
      }
      {
        match: "(&|§)1"
        name: "keyword.type.mccolor.dark_blue.skript"
      }
      {
        match: "(&|§)2"
        name: "keyword.type.mccolor.dark_green.skript"
